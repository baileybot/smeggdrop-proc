{} {set igg [split [wget "https://www.indiegogo.com/projects/south-side-hackerspace-chicago"] \n]; foreach p $igg {if {[string match *currency-xlarge* $p]} {set total $p}}; set total [regexp -inline {(<span>.*</span>){1,1}?} $total ]] ; set total [regsub -all {(<.*span>){1,1}?} $total ""] ; lindex total 1 }
